datasource db {
  provider = "sqlite"
  url      = "file:./news.db"
}

generator client {
  provider = "prisma-client-js"
}

model NewsItem {
  id        String   @id @default(uuid())
  title     String
  body      String
  source    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}